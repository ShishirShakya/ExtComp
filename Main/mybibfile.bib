@article{Alizadeh2002,
abstract = {We propose using the price range in the estimation of stochastic volatility models. We show theoretically, numerically, and empirically that range-based volatility prox- ies are not only highly efficient, but also approximately Gaussian and robust to mi- crostructure noise. Hence range-based Gaussian quasi-maximum likelihood estimation produces highly efficient estimates of stochastic volatility models and extractions of latent volatility.We use our method to examine the dynamics of daily exchange rate volatility and find the evidence points strongly toward two-factor models with one highly persistent factor and one quickly mean-reverting factor. VOLATILITY},
